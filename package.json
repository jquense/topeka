    "roots": [
      "test"
    ],
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "devDependencies": {
    "@4c/cli": "^2.0.1",
    "@4c/semantic-release-config": "^2.0.5",
    "@4c/tsconfig": "^0.3.1",
