    "react-dom": "^16.12.0",
    "react-overlays": "^2.1.1",
    "rimraf": "^3.0.2",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.4.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-atoms": "^12.1.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
