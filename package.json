{
  "name": "topeka",
  "version": "3.0.2",
  "description": "Idiomatic, two-way bindings, in React",
  "main": "index.js",
  "modules": "es/index.js",
  "scripts": {
    "test": "jest",
    "tdd": "jest --watch",
    "lint": "eslint test src",
    "docs": "rimraf ./docs/bundle.js && webpack-dev-server --config docs/webpack.config.js --mode production",
    "build:docs": "webpack --config docs/webpack.config.js",
    "build:esm": " babel src --out-dir lib/es --delete-dir-on-start --env-name esm",
    "build": "babel src --out-dir lib --delete-dir-on-start && npm run build:esm",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jquense/topeka.git"
  },
  "keywords": [
    "react",
    "binding",
    "input",
    "form",
    "valueLink"
  ],
  "author": "Jason Quense <monastic.panic@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jquense/topeka/issues"
  },
  "homepage": "https://github.com/jquense/topeka#readme",
  "jest": {
    "roots": [
      "test"
    ],
    "setupTestFrameworkScriptFile": "./test/setup.js"
  },
  "devDependencies": {
    "@4c/semantic-release-config": "^1.0.2",
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@monastic.panic/component-playground": "^3.0.0",
    "@monastic.panic/enzyme-adapter-react-16": "^1.2.1",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-preset-jason": "^4.1.1",
    "chai": "^4.1.2",
    "component-metadata-loader": "^4.0.0",
    "dirty-chai": "^2.0.1",
    "enzyme": "^3.3.0",
    "eslint": "^4.19.1",
    "eslint-config-jason": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^23.1.0",
    "lodash": "^4.17.10",
    "marked": "^0.4.0",
    "mocha": "^5.1.1",
    "prop-types": "^15.6.1",
    "raw-loader": "^0.5.1",
    "react": "^16.3.2",
    "react-bootstrap": "^0.32.1",
    "react-component-metadata": "^3.1.0",
