{
  "name": "topeka",
  "version": "4.4.2",
  "description": "Idiomatic, two-way bindings, in React",
  "main": "lib/index.js",
  "module": "lib/es/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "tdd": "jest --watch",
    "lint": "eslint test src --ext .ts --ext .tsx --ext .js",
    "docs": "rimraf ./docs/bundle.js && webpack-dev-server --config docs/webpack.config.js --mode production",
    "build:docs": "webpack --config docs/webpack.config.js",
    "build": "4c build src -d lib",
    "release": "rollout",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jquense/topeka.git"
  },
  "keywords": [
    "react",
    "binding",
    "input",
    "form",
    "valueLink"
  ],
  "author": {
    "name": "Jason Quense",
    "email": "monastic.panic@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jquense/topeka/issues"
  },
  "homepage": "https://github.com/jquense/topeka#readme",
  "jest": {
    "roots": [
      "test"
    ],
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "publishConfig": {
    "directory": "lib"
  },
  "devDependencies": {
    "@4c/cli": "^2.2.0",
    "@4c/semantic-release-config": "^2.0.14",
    "@4c/tsconfig": "^0.3.1",
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@monastic.panic/component-playground": "^3.0.0",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^26.6.3",
    "babel-preset-env-modules": "^1.0.0",
    "chai": "^4.3.1",
    "dirty-chai": "^2.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.21.0",
    "eslint-config-4catalyzer-typescript": "^3.0.3",
    "eslint-config-jason": "^8.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "lodash": "^4.17.21",
    "marked": "^2.0.1",
    "mocha": "^8.3.0",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.2",
    "react": "^16.13.0",
    "react-bootstrap": "^1.5.1",
    "react-dom": "^16.13.0",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.5.0",
    "typescript": "^4.2.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {
    "@restart/hooks": "^0.3.26",
    "property-expr": "^2.0.4",
    "uncontrollable": "^7.2.1"
  },
  "release": {
    "conventionalCommits": true
  }
}
