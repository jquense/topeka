    "semantic-release": "^15.3.1",
    "sinon": "^5.0.6",
    "sinon-chai": "^3.0.0",
    "travis-deploy-once": "^5.0.0",
    "webpack": "^4.10.2",
    "webpack-atoms": "^7.0.0",
    "webpack-cli": "^3.0.1",
    "webpack-dev-server": "^3.1.4"
  },
  "peerDependencies": {
    "react": ">=16.3.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.49",
    "classnames": "^2.2.5",
    "invariant": "^2.2.4",
    "property-expr": "^1.3.1",
    "react-lifecycles-compat": "^3.0.2",
    "uncontrollable": "^6.0.0"
