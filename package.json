    "build:docs": "webpack --config docs/webpack.config.js",
    "build": "4c build src -d lib",
    "release": "rollout",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jquense/topeka.git"
  },
  "keywords": [
    "react",
    "binding",
    "input",
    "form",
    "valueLink"
