{
  "name": "topeka",
  "version": "4.2.0",
  "description": "Idiomatic, two-way bindings, in React",
  "main": "lib/index.js",
  "module": "lib/es/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "tdd": "jest --watch",
    "lint": "eslint test src --ext .ts, .tsx, .js",
